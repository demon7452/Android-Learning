        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.textviewtest.MainTest"
            android:label="@string/title_activity_main_test" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.textviewtest.Show_richText"
            android:label="@string/title_activity_show_rich_text" >
        </activity>
        <activity
            android:name="com.example.textviewtest.Text_image"
            android:label="@string/title_activity_text_image" >
        </activity>
        <activity
            android:name="com.example.textviewtest.LinkActivity"
            android:label="@string/title_activity_link" >
        </activity>
        <activity
            android:name="com.example.textviewtest.Text_background"
            android:label="@string/title_activity_text_background" >
        </activity>
    </application>

</manifest>
